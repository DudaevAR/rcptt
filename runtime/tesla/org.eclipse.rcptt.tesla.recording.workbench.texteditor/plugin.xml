<plugin>
   <extension
         point="org.eclipse.rcptt.tesla.recording.core.recorder">
      <recorder
            class="org.eclipse.rcptt.tesla.recording.jface.text.WorkbenchTexteditorRecordingProcessor">
      </recorder>
   </extension>

