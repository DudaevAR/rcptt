------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select "Q7Project/TC1" | get-menu "Export/Q7 Portable Format" | click
get-window "Export" | get-button Finish | click
get-view "Q7 Explorer" | get-tree | select Q7Project3 | get-menu "Import/Q7 Portable Format" | click
get-window -from "Window.createShell()" | get-editbox -after [get-label "Testcase name:"] | get-property text 
    | equals TC1 | verify-true
get-window -from "Window.createShell()" | get-button Finish | click
get-view "Q7 Explorer" | get-tree | get-item "Q7Project3/TC1" | get-property caption | equals TC1 | verify-true
get-view "Q7 Explorer" | get-tree | select "Q7Project3/TC1" | double-click -nowait
