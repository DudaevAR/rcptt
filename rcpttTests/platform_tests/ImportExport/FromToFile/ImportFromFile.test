------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select "Q7Project/TC1" | get-menu "Export/Q7 Portable Format" | click
get-window "Export" | get-button "Export to File" | click
set-dialog-result File "platform:/resource/qqq.test"
with [get-window "Export"] {
    get-button "Browse..." | click
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | select Q7Project3 | get-menu "Import/Q7 Portable Format" | click

with [get-window "Import"] {
	get-button "Import from File" | click
	set-dialog-result File "platform:/resource/qqq.test"
    get-button "Browse..." -index 1 | click
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | get-item Q7Project3 | get-property childCount | equals 8 | verify-true
get-view "Q7 Explorer" | get-tree | select "Q7Project3/TC1" | double-click -nowait
