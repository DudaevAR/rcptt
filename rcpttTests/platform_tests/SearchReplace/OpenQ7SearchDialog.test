------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-button Search | click -arrow
get-menu "1 Q7 Search" | click
get-window Search | get-tab-folder | get-property activePage | equals "Q7 Search" | verify-true
get-window Search | get-button Cancel | click
get-view "Q7 Explorer" | get-tree | key-type [get-by-os -default "M1+h" -macosx "M4+h"]
with [get-window Search] {
    with [get-tab-folder] {
        get-tab-item "File Search" | click
        get-tab-item "Q7 Search" | click
    }
    get-button Cancel | click
}
