get-view "Test Explorer" | get-tree | get-menu "New/Project..." | click
with [get-window "New Project"] {
    get-tree | select "General/Project"
    get-button "Next >" | click
    get-editbox -after [get-label "Project name:"] | set-text assertData
    get-button Finish | click
}
get-view "Test Explorer" | get-tree | select "Q7Project/CheckSetCommand" | double-click
with [get-editor CheckSetCommand | get-section Contexts] {
    click
    get-button "Add..." | click
}
with [get-window "Select context"] {
	get-table | get-table-data | write-csv-file "workspace:/assertData/table.csv"
	get-button Cancel | click
}
get-view "Test Explorer" | get-tree | select "Q7Project/Default" | key-type Del
get-window "Delete Resources" | get-button OK | click
with [get-editor CheckSetCommand | get-section Contexts] {
    click
    get-button "Add..." | click
}
get-window "Select context" | get-table | get-table-data | eq [read-csv-file "workspace:/assertData/table.csv"] | not | assert-true
