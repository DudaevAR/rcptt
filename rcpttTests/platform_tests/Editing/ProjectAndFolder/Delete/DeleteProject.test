------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select p2 | get-menu Delete | click
with [get-window "Delete Resources"] {
    get-button "Delete project contents on disk (cannot be undone)" | click
    get-button OK | click
}
get-view "Q7 Explorer" | get-tree | get-property itemCount | equals 2 | verify-true
get-view "Q7 Explorer" | get-tree | get-item p1 | get-property caption | equals p1 | verify-true
get-view "Q7 Explorer" | get-tree | get-item p3 | get-property caption | equals p3 | verify-true
with [get-view "Q7 Explorer" | get-tree] {
    select p1 p3
    get-menu Delete | click
}
with [get-window "Delete Resources"] {
    get-button "Delete project contents on disk (cannot be undone)" | click
    get-button OK | click
}
get-view "Q7 Explorer" | get-tree | get-property itemCount | equals 0 | verify-true
