------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | get-menu "New/Project..." | click
with [get-window "New Project"] {
    with [get-editbox -after [get-label "Wizards:"]] {
        set-text Q7
        key-type "M1+a"
        key-type "M1+c"
    }
    get-tree | select "Q7/Q7 Project"
    get-button "Next >" | click
    with [get-editbox -after [get-label "Project name:"]] {
        key-type "M1+v"
    }
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | select Q7
with [get-view "Q7 Explorer" | get-tree] {
    get-property itemCount | equals 1 | verify-true
    get-item Q7 | get-property caption | equals Q7 | verify-true
}
