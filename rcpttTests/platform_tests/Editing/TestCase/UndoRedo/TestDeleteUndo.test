------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

with [get-view "Q7 Explorer" | get-tree] {
    select "Q7Project/context" "Q7Project/scenario"
    get-menu Delete | click
}
get-window "Delete Resources" | get-button OK | click
get-view "Q7 Explorer" | get-tree | select Q7Project | key-type "M1+z"
get-view "Q7 Explorer" | get-tree | get-item "Q7Project/context" | get-property caption | equals context | verify-true
get-view "Q7 Explorer" | get-tree | get-item "Q7Project/scenario" | get-property caption | equals scenario | verify-true

get-view "Q7 Explorer" | get-tree | key-type "M1+y"
get-view "Q7 Explorer" | get-tree | verify-error { get-item "Q7Project/context" }
get-view "Q7 Explorer" | get-tree | verify-error { get-item "Q7Project/scenario" }
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
