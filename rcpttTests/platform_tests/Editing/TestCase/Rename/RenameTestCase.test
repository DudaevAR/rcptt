------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select TestQ7Prj | get-menu "New/Test Case" | click
with [get-window "New Test Case"] {
    get-editbox -after [get-label "Name:"] | set-text "Initial Name"
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | get-item "TestQ7Prj/Initial Name" | get-property caption | equals "Initial Name" 
    | verify-true
get-view "Q7 Explorer" | get-tree | select "TestQ7Prj/Initial Name" | key-type F2
with [get-window "Rename Resource"] {
    get-editbox -after [get-label "New name:"] | set-text "New Name with Special Symbols /_\\"
    get-button OK | click
}
get-view "Q7 Explorer" | get-tree | get-item "TestQ7Prj/New Name with Special Symbols \\/_\\" | get-property caption 
    | equals "New Name with Special Symbols /_\\" | verify-true
get-editor "New Name with Special Symbols /_\\" | get-editbox -after [get-label "Name:"] | get-property rawValue 
    | equals "New Name with Special Symbols /_\\" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
