------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select "MyProject/P1" | double-click
with [get-editor P1 | get-section Parameters | get-table] {
    select p1 | activate-cell-edit
    apply-cell-edit -deactivate
    select p3 p1
    get-menu "Copy selected parameters" | click
}
get-view "Q7 Explorer" | get-tree | select "MyProject/P2" | double-click
get-editor P2 | get-section Parameters | get-table | get-menu Paste | click
get-editor P1 | click
get-editor P2 | click
with [get-editor P2 | get-section Parameters | get-table] {
    get-property itemCount | equals 3 | verify-true
    get-item p1 | get-property "columns[0]" | equals p1 | verify-true
    get-item p1 | get-property "columns[1]" | equals v1 | verify-true
    get-item p3 | get-property "columns[0]" | equals p3 | verify-true
    get-item p3 | get-property "columns[1]" | equals v3 | verify-true
}
