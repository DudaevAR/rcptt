------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select TestQ7Prj | get-menu "New/Context" | click
with [get-window "New Context Wizard"] {
    get-table | select Workspace
    get-editbox -after [get-label "Name:"] | set-text Ws
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | get-item "TestQ7Prj/Ws" | get-property caption | equals Ws | verify-true
get-tab-folder -index 1 | get-property activePage | equals Ws | verify-true
