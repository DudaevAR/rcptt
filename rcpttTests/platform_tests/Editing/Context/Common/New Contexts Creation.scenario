------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select TestQ7Prj | get-menu "New/Context" | click
with [get-window "New Context Wizard"] {
    get-editbox -after [get-label "Name:"] | set-text WSCtx
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | select TestQ7Prj | get-menu "New/Context" | click
with [get-window "New Context Wizard"] {
    get-editbox -after [get-label "Name:"] | set-text WBCtx
    get-table | select Workbench
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | select TestQ7Prj | get-menu "New/Context" | click
with [get-window "New Context Wizard"] {
    get-table | select Preferences
    get-editbox -after [get-label "Name:"] | set-text PrefCtx
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | select TestQ7Prj | get-menu "New/Context" | click
with [get-window "New Context Wizard"] {
    get-editbox -after [get-label "Name:"] | set-text GroupCtx
    get-table | select Group
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | select TestQ7Prj | get-menu "New/Context" | click
with [get-window "New Context Wizard"] {
    get-table | select "Ecl Script"
    get-editbox -after [get-label "Name:"] | set-text EclCtx
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | select TestQ7Prj | get-menu "New/Context" | click
with [get-window "New Context Wizard"] {
    get-table | select Launch
    get-editbox -after [get-label "Name:"] | set-text DebugCtx
    get-button Finish | click
}
get-view "Q7 Explorer" | get-tree | get-item "TestQ7Prj/DebugCtx" | get-property caption | equals DebugCtx | verify-true
get-view "Q7 Explorer" | get-tree | get-item "TestQ7Prj/EclCtx" | get-property caption | equals EclCtx | verify-true
get-view "Q7 Explorer" | get-tree | get-item "TestQ7Prj/GroupCtx" | get-property caption | equals GroupCtx | verify-true
get-view "Q7 Explorer" | get-tree | get-item "TestQ7Prj/PrefCtx" | get-property caption | equals PrefCtx | verify-true
get-view "Q7 Explorer" | get-tree | get-item "TestQ7Prj/WBCtx" | get-property caption | equals WBCtx | verify-true
get-view "Q7 Explorer" | get-tree | get-item "TestQ7Prj/WSCtx" | get-property caption | equals WSCtx | verify-true
