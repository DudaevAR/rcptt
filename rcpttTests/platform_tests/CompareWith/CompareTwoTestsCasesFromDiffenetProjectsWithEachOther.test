------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

with [get-view "Test Explorer" | get-tree] {
    select "Q7Project1/TestCase2" "Q7Project2/TestCase3"
    get-menu "Compare With/Each Other" | click
}
with [get-editor "Compare (\'Q7Project1/TestCase2.test\' - \'Q7Project2/TestCase3.test\')" | get-tree] {
    get-property itemCount | equals 1 | verify-true
    get-item "Test Case" | get-property caption | equals "Test Case" | verify-true
    get-item "Test Case/Description" | get-property caption | equals Description | verify-true
    get-item "Test Case/Name" | get-property caption | equals Name | verify-true
    get-item "Test Case/Script" | get-property caption | equals Script | verify-true
    get-item "Test Case/Tags" | get-property caption | equals Tags | verify-true
}
get-editor "Compare (\'Q7Project1/TestCase2.test\' - \'Q7Project2/TestCase3.test\')" | get-tree 
    | select "Test Case/Description" | double-click
with [get-editor "Compare (\'Q7Project1/TestCase2.test\' - \'Q7Project2/TestCase3.test\')"] {
    get-editbox -after [get-label "Q7Project2/TestCase3.test"] | get-property value | equals description2 | verify-true
    get-editbox -index 1 | get-property value | equals description3 | verify-true
}
get-editor "Compare (\'Q7Project1/TestCase2.test\' - \'Q7Project2/TestCase3.test\')" | get-tree 
    | select "Test Case/Name" | double-click
with [get-editor "Compare (\'Q7Project1/TestCase2.test\' - \'Q7Project2/TestCase3.test\')"] {
    get-editbox -after [get-label "Q7Project2/TestCase3.test"] | get-property value | equals TestCase2 | verify-true
    get-editbox -index 1 | get-property value | equals TestCase3 | verify-true
}
get-editor "Compare (\'Q7Project1/TestCase2.test\' - \'Q7Project2/TestCase3.test\')" | get-tree 
    | select "Test Case/Script" | double-click
with [get-editor "Compare (\'Q7Project1/TestCase2.test\' - \'Q7Project2/TestCase3.test\')"] {
    get-editbox -after [get-label "Q7Project2/TestCase3.test"] | get-property value | equals script2 | verify-true
    get-editbox -index 1 | get-property value | equals script3 | verify-true
}
get-editor "Compare (\'Q7Project1/TestCase2.test\' - \'Q7Project2/TestCase3.test\')" | get-tree 
    | select "Test Case/Tags" | double-click
with [get-editor "Compare (\'Q7Project1/TestCase2.test\' - \'Q7Project2/TestCase3.test\')"] {
    get-editbox -after [get-label "Q7Project2/TestCase3.test"] | get-property value | equals tag2 | verify-true
    get-editbox -index 1 | get-property value | equals tag3 | verify-true
}
