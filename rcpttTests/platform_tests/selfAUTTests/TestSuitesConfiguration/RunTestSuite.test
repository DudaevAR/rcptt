------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-menu "Run/Run Configurations..." | click
get-window "Run Configurations" | get-tree | select "Q7 Test Suite/Q7Project" "Q7 Test Suite/Q7Project2"
with [get-window "Run Configurations"] {
    get-button Run | get-property enablement | equals false | verify-true
    get-button Close | get-property enablement | equals true | verify-true
}
with [get-window "Run Configurations"] {
    get-tree | select "Q7 Test Suite/Q7Project2"
    get-button Run | click
}
wait 100
get-view "Execution View" | get-tree | get-property itemCount | equals 6 | verify-true
get-view "Q7 Explorer" | get-tree | select "ProjectWithTestSuites/Q7Project.launch" | get-menu "Run As/.* Q7Project" 
    | click
wait 100
get-view "Execution View" | get-tree | get-property itemCount | equals 5 | verify-true
get-view "Q7 Explorer" | get-tree | select "ProjectWithTestSuites/Test Suite.launch"
get-button Run | click
wait 100
get-view "Execution View" | get-tree | get-property itemCount | equals 7 | verify-true
get-button Run | click -arrow
get-menu ".*Q7Project2" | click
wait 100
get-view "Execution View" | get-tree | get-property itemCount | equals 6 | verify-true

------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
