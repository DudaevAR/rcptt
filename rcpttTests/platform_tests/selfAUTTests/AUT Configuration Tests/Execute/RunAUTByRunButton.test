with [get-view Applications] {
    click
    get-tree | select selfQ7
    get-button Stop | click
    get-tree | select selfQ7
    get-button Run | click
}
get-button "Record a Snippet" | click
get-aut selfQ7 | eval {
	get-advanced-info
} | report-append
get-window "Control Panel - selfQ7 [Recording]" | get-button Stop | click
get-window "Control Panel - selfQ7" | get-button "Return to RCPTT" | click
get-view Console | get-button Terminate | click
get-view Applications | click
