------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select "q7project/simpleTest1" | double-click
with [get-editor simpleTest1 | get-section Contexts] {
    click
    get-link "<a>Configure Project Default Contexts</a>" | click-link
}
with [get-tab-folder -index 1] {
    get-property activePage | equals "q7project - Project Settings" | verify-true
    get-property tabCount | equals 2 | verify-true
}
get-view "Q7 Explorer" | get-button "Link with Editor" | click
get-view "Q7 Explorer" | get-tree | get-item "q7project/Project Settings" | get-property selected | equals true | verify-true
