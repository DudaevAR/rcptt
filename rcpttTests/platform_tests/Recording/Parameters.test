------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

set-param New "Name of the Project"
with [get-view "Test Explorer"] {
    click
    get-tree | select TestQ7Prj | get-menu "Rename..." | click
}
with [get-window "Rename Resource"] {
    get-editbox -after [get-label "New name:"] | set-text [get-param New]
    get-button OK | click
}
with [get-view "Test Explorer" | get-tree] {
    get-item [get-param New] | get-property caption | equals [get-param New] | verify-true
    get-property itemCount | equals 1 | verify-true
}
