    get-editor EclContext | get-editbox -after [get-label "Name:"] | key-type End
    verify-error { get-window "Problem Occurred" }
} -allowStatusDialog

exec-with-options {
    get-editor EclContext | get-editbox -after [get-label "Name:"] | key-type "M1+ARROW_RIGHT"
    verify-error { get-window "Problem Occurred" }
} -allowStatusDialog

