------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Test Explorer" | get-tree | select MyProject | get-menu "New/Verification" | click
with [get-window "New Verification Wizard"] {
    get-editbox -after [get-label "Name:"] | set-text v1
    get-table | select "Error Log"
    get-button Finish | click
}
get-tab-folder -index 1 | get-property activePage | equals v1 | verify-true
with [get-editor v1] {
    get-section Require | get-property "getText()" | equals Require | verify-true
    get-section Allow | get-property "getText()" | equals Allow | verify-true
    get-section Deny | get-property "getText()" | equals Deny | verify-true
}
