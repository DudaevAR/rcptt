------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Test Explorer" | get-tree | select "MyProject/ErrorLogVerification" | double-click
with [get-editor ErrorLogVerification | get-section Allow | get-table] {
    select "org.eclipse.ui" -column "Plugin Pattern" | activate-cell-edit -column 5
    get-editbox | set-text " 3"
    apply-cell-edit -deactivate
    select "org.eclipse.equinox" -column "Plugin Pattern" | activate-cell-edit -column 5
    get-editbox | set-text "2 3 4 5"
    apply-cell-edit -deactivate
    select [get-item "org.eclipse.pde" -column "Plugin Pattern" -index 1] | activate-cell-edit -column 5
    get-editbox | set-text "   1"
    apply-cell-edit -deactivate
}
with [get-editor ErrorLogVerification | get-section Allow | get-table] {
    get-item "org.eclipse.ui" -column "Plugin Pattern" | get-property "columns[5]" | equals 3 | verify-true
    get-item "org.eclipse.equinox" -column "Plugin Pattern" | get-property "columns[5]" | equals 2345 | verify-true
    get-item "org.eclipse.pde" -column "Plugin Pattern" -index 1 | get-property "columns[5]" | equals 1 | verify-true
}
