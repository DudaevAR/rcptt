------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Test Explorer" | get-tree | select "MyProject/ErrorLogVerification" | double-click
get-editor ErrorLogVerification | get-section Allow | get-table | select [get-item "org.eclipse.pde" 
    -column "Plugin Pattern" -index 2] | get-menu Remove | click
get-editor ErrorLogVerification | get-section Allow | get-table | get-property itemCount | equals 4 | verify-true
with [get-editor ErrorLogVerification | get-section Allow] {
    get-table | select "org.eclipse.ui" -column "Plugin Pattern" | activate-cell-edit -column 4
    get-button Remove | click
}
get-editor ErrorLogVerification | get-section Allow | get-table | get-property itemCount | equals 3 | verify-true
with [get-editor ErrorLogVerification | get-section Allow] {
    with [get-table] {
        select "org.eclipse.pde" -column "Plugin Pattern" | activate-cell-edit -column 4
        apply-cell-edit -deactivate
        select [get-item "org.eclipse.pde" -column "Plugin Pattern"] [get-item "org.eclipse.equinox" 
            -column "Plugin Pattern"] [get-item "org.eclipse.pde" -column "Plugin Pattern" -index 1]
    }
    get-button Remove | click
}
get-editor ErrorLogVerification | get-section Allow | get-table | get-property itemCount | equals 0 | verify-true
