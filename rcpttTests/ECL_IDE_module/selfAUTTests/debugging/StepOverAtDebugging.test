------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Test Explorer" | get-tree | select "MyProject/CreateJavaProject" | double-click
with [get-editor CreateJavaProject | get-section Script | get-left-ruler] {
    get-ruler-column -index 1 | double-click-ruler 1
    hover-ruler -line 1
}
get-view "Test Explorer" | get-tree | select "MyProject/CreateJavaProject" | get-menu "Debug As/.* Test Cases" | click
get-view Debug | get-tree 
    | get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/get-view \\(CreateJavaProject: 1\\)" 
    | get-property caption | equals "get-view (CreateJavaProject: 1)" | verify-true
get-view Debug | get-button "Step Over (F6)" | click
with [get-view Debug | get-tree] {
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread" | get-property childCount | equals 2 
        | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/get-window \\(CreateJavaProject: 2\\)" 
        | get-property caption | equals "get-window (CreateJavaProject: 2)" | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/with \\(CreateJavaProject: 2\\)" | get-property caption 
        | equals "with (CreateJavaProject: 2)" | verify-true
}
get-view Debug | get-tree | key-type F6
with [get-view Debug | get-tree] {
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread" | get-property childCount | equals 2 
        | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/get-tree \\(CreateJavaProject: 3\\)" 
        | get-property caption | equals "get-tree (CreateJavaProject: 3)" | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/with \\(CreateJavaProject: 2\\)" | get-property caption 
        | equals "with (CreateJavaProject: 2)" | verify-true
}
get-view Debug | get-button "Step Over (F6)" | click
with [get-view Debug | get-tree] {
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread" | get-property childCount | equals 2 
        | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/get-button \\(CreateJavaProject: 4\\)" 
        | get-property caption | equals "get-button (CreateJavaProject: 4)" | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/with \\(CreateJavaProject: 2\\)" | get-property caption 
        | equals "with (CreateJavaProject: 2)" | verify-true
}
get-view Debug | get-tree | key-type F6
with [get-view Debug | get-tree] {
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread" | get-property childCount | equals 3 
        | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/get-label \\(CreateJavaProject: 5\\)" 
        | get-property caption | equals "get-label (CreateJavaProject: 5)" | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/get-editbox \\(CreateJavaProject: 5\\)" 
        | get-property caption | equals "get-editbox (CreateJavaProject: 5)" | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/with \\(CreateJavaProject: 2\\)" | get-property caption 
        | equals "with (CreateJavaProject: 2)" | verify-true
}
get-view Debug | get-button "Resume (F8)" | click
get-button "Q7 perspective" | click
wait 1000
with [get-view "Execution View"] {
    with [get-tree] {
        get-property itemCount | equals 1 | verify-true
        get-item "CreateJavaProject.*" | get-property caption | equals "CreateJavaProject.*" 
            | verify-true
    }
    get-label "1/1" | get-property caption | equals "1/1" | verify-true
    get-label 0 | get-property caption | equals 0 | verify-true
    get-label 0 -after [get-label "Stops:"] | get-property caption | equals 0 | verify-true
}
