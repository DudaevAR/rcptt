    | get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/get-property \\(CreateJavaProject: 8\\)" 
    | get-property caption | equals "get-property (CreateJavaProject: 8)" | verify-true
get-view Debug | get-tree | key-type F5
get-view Debug | get-tree 
    | get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/equals \\(CreateJavaProject: 8\\)" 
    | get-property caption | equals "equals (CreateJavaProject: 8)" | verify-true
get-view Debug | get-tree | key-type F5
get-view Debug | get-tree 
    | get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/verify-true \\(CreateJavaProject: 8\\)" 
