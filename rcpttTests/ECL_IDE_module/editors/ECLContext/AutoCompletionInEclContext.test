------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Test Explorer" | get-tree | select "MyProject/Open Modal Dialog" | double-click
with [get-editor "Open Modal Dialog" | get-section Script | get-text-viewer] {
    set-caret-pos 2 41
    key-type Enter
    type-text "get-"
    wait 1000
    key-type "Ctrl+SPACE"
    type-text w
    key-type Enter
    type-text " \"Test "
    type-text "\" | cl"
    key-type "Ctrl+SPACE"
    key-type Down -times 7
    key-type Enter -times 2
    key-type "M1+s"
}

get-editor "Open Modal Dialog" | get-section Script | get-editbox | get-property text | equals "// Task in ECL-context\n"
    + "get-menu \"File/New/Java Project\" | click\n"
    + "get-window \"Test \" | close" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
