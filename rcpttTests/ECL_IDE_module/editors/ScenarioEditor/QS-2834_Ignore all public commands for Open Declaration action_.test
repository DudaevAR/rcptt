------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select "MyProject/Dummy" | double-click
with [get-editor Dummy | get-section Script | get-text-viewer] {
    set-caret-pos 1 67
    key-type Enter
    type-text "get-"
    show-content-assist
    type-text window
    set-caret-pos 3 1
    hover-text 1 9
    set-caret-pos 1 5
    hover-text 1 9
    get-menu "Open Declaration" | click
}
get-editor "New Java Class Procs" | get-section Script | get-text-viewer | get-property "getSelectionText()" 
    | equals "create-java-class" | verify-true
with [get-editor Dummy] {
    click
    with [get-section Script | get-text-viewer] {
        set-caret-pos 2 8
        hover-text 2 6
        set-caret-pos 3 1
 		get-menu "Open Declaration" | get-property enablement | equals false | verify-true
    }
}
