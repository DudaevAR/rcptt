------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select "MyProject/New Java Class Procs" | double-click
with [get-editor "New Java Class Procs" | get-section Script | get-text-viewer] {
    hover-text 30 23
    set-caret-pos 30 23
    open-declaration
}
get-editor JavaLabels | get-section Parameters | get-table | get-item newClassTitle | get-property selected 
    | equals true | verify-true
get-tab-folder -index 1 | get-property activePage | equals JavaLabels | verify-true
